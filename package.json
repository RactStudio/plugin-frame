{
  "name": "plugin-frame",
  "version": "0.9.2",
  "description": "Plugin Frame: A modern WordPress plugin development framework with Composer, npm, Tailwind, Twig, and Laravel-like structure. Ideal for lightweight or complex plugins. Complies with WP & PSR-4 standards. Modular, scalable, and dev-friendly.",
  "main": "plugin-frame.php",
  "scripts": {
    "tunnel": "bash ./pf/cf-tunnel.sh",
    "ngrok": "bash ./pf/ngrok-tunnel.sh",
    "pf": "node ./pf/pf.mjs",
    "watch": "grunt watch",
    "translate-wp": "wp i18n make-pot . languages/plugin-frame.pot --exclude=node_modules,tests,.dist,vendor --allow-root",
    "translate-twig": "node pf/translate-twig.mjs",
    "translate-gen": "node pf/translate-gen.mjs",
    "generate-pomo": "node pf/translate-po-mo.mjs",
    "translate": "npm run translate-wp && npm run translate-twig && npm run translate-gen && npm run generate-pomo",
    "tailwind": "npx tailwindcss -i ./resources/assets/css/tailwind.css -o ./resources/assets/css/tailwind.min.css --minify",
    "tailwind-watch": "npx tailwindcss -i ./resources/assets/css/tailwind.css -o ./resources/assets/css/tailwind.min.css --minify --watch",
    "build": "npm run translate && npm run tailwind && grunt",
    "build:prod": "composer dump-autoload && npm run translate && npm run tailwind && grunt build:prod && npm run pf"
  },
